{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "sourceMap": true,
    "declaration": false,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Bundler",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "isolatedModules": false,
    "importHelpers": true,
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["./lib/*"],
      "@/components": ["lib/components/index.ts"],
      "@/container": ["lib/repository/Services/container.ts"],
      "@/helpers": ["lib/helpers/index.ts"],
      "@/hooks": ["lib/hooks/index.ts"],
      "@/interfaces": ["lib/repository/Interfaces/index.ts"],
      "@/layouts": ["lib/layouts/index.tsx"],
      "@/redux": ["lib/redux/store.ts"],
      "@/repository": ["lib/repository/Interfaces/index.ts"],
      "@/routes": ["lib/routes/index.ts"],
      "@/templates/forms": ["lib/templates/Forms/index.ts"],
      "@/teste": ["lib/teste/lib/index.ts"],
      "@/themes/babbo": ["lib/themes/babbo/index.ts"],
      "@/themes/default": ["lib/themes/default.tsx"],
      "@/tools": ["lib/tools/index.ts"],
      "@/types": ["lib/repository/Types/_index.ts"],
      "@/validators": ["lib/validators/index.ts"],
      "@babbo/validator": ["validator/src/index.ts"],
      "@babbo/validator/server": ["validator/src/server.ts"],
      "@/services": ["lib/repository/Services/index.ts"],
      "@/entities": ["lib/repository/Entities/index.ts"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
