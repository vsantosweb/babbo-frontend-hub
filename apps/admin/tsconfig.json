{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": ["jest", "node"],
    "paths": {
      "@/components": ["../../lib/components/index.ts"],
      "@/container": ["../../lib/repository/Services/container.ts"],
      "@/helpers": ["../../lib/helpers/index.ts"],
      "@/hooks": ["../../lib/hooks/index.ts"],
      "@/interfaces": ["../../lib/repository/Interfaces/index.ts"],
      "@/layouts": ["../../lib/layouts/index.tsx"],
      "@/repository": ["../../lib/repository/Interfaces/index.ts"],
      "@/routes": ["../../lib/routes/index.ts"],
      "@/templates/forms": ["../../lib/templates/Forms/index.ts"],
      "@/teste": ["../../lib/teste/../../lib/index.ts"],
      "@/themes/babbo": ["../../lib/themes/babbo/index.ts"],
      "@/themes/default": ["../../lib/themes/default.tsx"],
      "@/tools": ["../../lib/tools/index.ts"],
      "@/types": ["../../lib/repository/Types/_index.ts"],
      "@/validators": ["../../lib/validators/index.ts"],
    },
  },
  "include": ["**/*.tsx", "**/*.js", "**/*.jsx", "next-env.d.ts", "next.d.ts"],
  "exclude": ["node_modules", "jest.config.ts", "**/*.spec.ts", "**/*.test.ts"]
}
